{
  "name": "@anion155/proposal-iterator-helpers",
  "version": "1.0.0",
  "description": "Polyfills implementing the Iterator Helpers proposal for ECMAScript.",
  "keywords": [
    "iterator",
    "iterable",
    "helpers",
    "ecmascript",
    "proposal",
    "polyfill"
  ],
  "author": {
    "name": "anion155",
    "url": "https://github.com/anion155"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/anion155/monorepo.git",
    "directory": "packages/proposal-iterator-helpers"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./global": "./src/global.ts",
    "./types": "./src/types.ts"
  },
  "!exports": [
    "**/*"
  ],
  "+exports": {
    ".": "./src/index.ts",
    "./global": "./src/global.ts",
    "./types": "./src/types.ts"
  },
  "scripts": {
    "lint": "eslint src && tsc --project ./tsconfig.build.json",
    "test": "jest src",
    "builder:ready": "rm dist/src/base.ts && cp ../polyfill-base/index.ts dist/src/base.ts",
    "prepublish": "generate.exports && builder",
    "publish": "cd dist && pnpm publish --access public"
  },
  "dependencies": {
    "@anion155/polyfill-base": "workspace:*"
  },
  "!dependencies": [
    "@anion155/polyfill-base"
  ],
  "devDependencies": {
    "@anion155/configs": "workspace:*",
    "@anion155/scripts": "workspace:*",
    "@jest/globals": "^30.0.0",
    "eslint": "^9.29.0",
    "jest": "^30.0.2",
    "prettier": "^3.5.3"
  }
}
